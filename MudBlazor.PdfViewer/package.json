{
  "name": "mudblazor.pdfviewer",
  "version": "1.1.0",
  "description": "A simple PDF viewer component for use with MudBlazor",
  "main": "index.js",
  "scripts": {
    "ts": "npx esbuild Ts/PdfViewer.ts --bundle --format=esm --target=esnext --outfile=wwwroot/mudpdfviewer.js --keep-names",
