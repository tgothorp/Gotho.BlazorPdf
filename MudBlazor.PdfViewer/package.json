  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tgothorp/MudBlazor.PdfViewer.git"
  },
  "keywords": [
    "blazor",
    "mudblazor",
    "pdf"
  ],
  "author": "tgothorp",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/tgothorp/MudBlazor.PdfViewer/issues"
  },
  "homepage": "https://github.com/tgothorp/MudBlazor.PdfViewer#readme",
  "devDependencies": {
    "clean-css-cli": "^5.6.3",
    "esbuild": "^0.25.0",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "pdfjs-dist": "^4.0.379",
    "print-js": "^1.6.0"
  }
}
